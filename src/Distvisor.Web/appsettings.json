{
  "ConnectionStrings": {
    "EventStore": "Host=localhost;Database=DistvisorES;Username=postgres;Password=mysecretpassword",
    "ReadStore": "Host=localhost;Database=DistvisorRS;Username=postgres;Password=mysecretpassword"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ClientConfig": {
    "Msal": {
      "Auth": {
        "ValidateAuthority": true
      },
      "Cache": {
        "CacheLocation": "localStorage"
      }
    }
  },
  "Finances": {
    "IsEmailPoolingEnabled": false,
    "EmailPoolingMinutesInterval":  5
  },
  "AllowedHosts": "*",
  "UseFakeApi": false
}
